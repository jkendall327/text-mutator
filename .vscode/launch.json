{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "name": "vuejs: firefox",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/frontend/src",
            "pathMappings": [
                {
                    "url": "http://localhost:5173/src",
                    "path": "${workspaceFolder}/frontend/src"
                }
            ]
        }
    ]
}